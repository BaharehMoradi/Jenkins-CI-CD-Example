pipeline {
    agent any
    
    stages {
        stage('Build') {
            steps {
                sh './mvnw clean'     //cleans the maven project
                sh './mvnw compile'
                sh './mvnw package'

            }
        }
        
        stage('Test') {
            steps {
                sh './mvnw test'
            }
        }
    }
}
    
